<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Kind</th>
      <th>Occurred at</th>
      <th>Data</th>
      <th>Created at</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.kind %></td>
        <td><%= event.occurred_at %></td>
        <td><%= hash_to_list event.data %></td>
        <td><%= event.created_at %></td>
        <td><%= link_to 'Show', event %></td>
      </tr>
    <% end %>
  </tbody>
</table>
